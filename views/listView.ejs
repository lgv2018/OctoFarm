
<% if(typeof printerCount != 'undefined' && printerCount > 0 ) {%>
  <div id="listView" class="row">
    <% if(clientSettings[0].listView.currentOp){%> <%-
    include('./partials/currentOperations.ejs') %> <% }%>
    <div class="col-lg-12 table-responsive-sm table-responsive-md">
      <table class="table table-dark text-center">
        <thead>
        <tr>
          <th class="sticky-table table-dark" scope="col">Sort Index</th>
          <th class="sticky-table table-dark" scope="col">Name</th>
          <th class="sticky-table table-dark" scope="col" width="300px">
            Control
          </th>
          <th class="sticky-table table-dark" scope="col" width="160px">
            Manage
          </th>
          <th class="sticky-table table-dark" scope="col" width="700px">Current File</th>
          <th class="sticky-table table-dark" scope="col" width="500px">
            Filament
          </th>
          <th class="sticky-table table-dark" scope="col">Status</th>
          <th class="sticky-table table-dark" scope="col" width="450px">
            Remaining Print Time
          </th>
          <th id="extraInfoPercent" class="sticky-table table-dark d-none" scope="col" width="100px">
            Percent Complete
          </th>
          <th id="extraInfoTitle" class="sticky-table table-dark d-none" scope="col" width="450px">
            ETA
          </th>
          <th class="sticky-table table-dark" scope="col" width="250px">
            Tool 0
          </th>
          <th class="sticky-table table-dark" scope="col" width="200px">Bed</th>
        </tr>
        </thead>
        <tbody id="listViewTable" data-jplist-group="printers">
        <%- include('./partials/Views/listView.ejs') %>
        </tbody>
      </table>
    </div>
  </div>

<% }else{ %>
  <%- include('./addPrinters.ejs') %>
<% } %>